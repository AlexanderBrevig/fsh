open Core
open Fsh

(* Helper to create a fresh state for each test *)
let fresh_state () =
  let state = Types.create_state () in
  Builtins.register_builtins state.dict;
  state
;;

(* All loop tests temporarily commented out for debugging *)
let%expect_test "dummy test" =
  let _ = fresh_state () in
  [%expect {| |}]
;;
